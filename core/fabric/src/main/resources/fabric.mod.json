{
	"schemaVersion": 1,
	"id": "${mod_id}",
	"version": "${version}",
	"name": "${mod_name} (Core)",
	"description": "The core module for smaller units, contains the majority of the code",
	"authors": [
		"${mod_author}"
	],
	"contact": {
		"sources": "https://github.com/GiantLuigi4/SU3x",
		"issues": "https://github.com/GiantLuigi4/SU3x/issues"
	},
	"license": "${license}",
	"icon": "assets/smallerunits/smaller_units.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"tfc.smallerunits.fabric.FabricMod"
		],
		"cardinal-components": [
			"tfc.smallerunits.fabric.ComponentRegistry"
		]
	},
	"mixins": [
		"${mod_id_dotted}.mixins.json",
		"${mod_id_dotted}.fabric.mixins.json"
	],
	"accessWidener": "${mod_id_dotted}.accesswidener",
	"depends": {
		"fabricloader": ">=0.14",
		"fabric": "*",
		"minecraft": "${minecraft_version}",
		"java": ">=17",
		"cardinal-components-chunk": "*",
		"cardinal-components-world": "*",
		"reach-entity-attributes": "*"
	},
	"custom": {
		"cardinal-components": [
			"smallerunits:unit_space_cap"
		],
		"modmenu": {
			"parent": {
				"id": "${mod_base_id}_fat",
				"name": "${mod_name}",
				"description": "${description}"
			}
		}
	}
}
