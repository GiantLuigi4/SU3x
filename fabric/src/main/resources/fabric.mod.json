{
	"schemaVersion": 1,
	"id": "${mod_id}",
	"version": "${version}",
	"name": "${mod_name} (Base)",
	"description": "The base module for SU, containing loader specific code",
	"authors": [
		"${mod_author}"
	],
	"contact": {
		"sources": "https://github.com/GiantLuigi4/SU3x",
		"issues": "https://github.com/GiantLuigi4/SU3x/issues"
	},
	"license": "${license}",
	"icon": "assets/smallerunits/smaller_units.png",
	"environment": "*",
	"mixins": [
		"${mod_id_dotted}.fabric.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.14",
		"fabric": "*",
		"minecraft": "${minecraft_version}",
		"java": ">=17",
		"cardinal-components-chunk": "*",
		"cardinal-components-world": "*",
		"reach-entity-attributes": "*"
	},
	"custom": {
		"modmenu": {
			"parent": {
				"id": "${mod_base_id}_fat",
				"name": "${mod_name}",
				"description": "${description}"
			}
		}
	}
}
  