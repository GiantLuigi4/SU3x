{
    "schemaVersion": 1,
	"id": "${mod_id}",
    "version": "${version}",
    "name": "${mod_name} (Lithium Support)",
    "description": "Compatibility for lithium/canary",
    "authors": [
        "${mod_author}"
    ],
	"contact": {
		"sources": "https://github.com/GiantLuigi4/SU3x",
		"issues": "https://github.com/GiantLuigi4/SU3x/issues"
	},
    "license": "${license}",
	"icon": "assets/smallerunits/smaller_units.png",
    "environment": "*",
    "mixins": [
        "${mod_id_dotted}.fabric.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=0.14",
        "minecraft": "${minecraft_version}",
        "java": ">=17"
    },
	"custom": {
		"modmenu": {
			"parent": {
				"id": "${mod_base_id}_fat",
				"name": "${mod_name}",
				"description": "${description}"
			}
		}
	}
}
  